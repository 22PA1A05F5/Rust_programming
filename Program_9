fn main() {
    const MAX_POINTS: u32 = 100_000;
    const PI: f64 = 3.141592653589793;
    const IS_ACTIVE: bool = true;
    const GREETING: &str = "Hello, Rust!";

    println!("MAX_POINTS: {}", MAX_POINTS);
    println!("PI: {}", PI);
    println!("IS_ACTIVE: {}", IS_ACTIVE);
    println!("GREETING: {}", GREETING);
}
